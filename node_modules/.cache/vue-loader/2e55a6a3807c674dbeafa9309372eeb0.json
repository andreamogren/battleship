Index) in 7\" :id=\"rowIndex + '-' + cellIndex\" @click=\"checkHit\"> <!-- :class=\"{ hit: hit, miss: miss }\" -->\n      </td>\n      </tr>\n\n    </table>\n    <h3>0 1 2 3 4 5 6</h3>\n  </div>\n  </template>\n\n  <script>\n  export default {\n  name: 'Battleship',\n  data() {\n    return {\n      rows: 7,\n      squares: 7,\n      firstNum: String,\n      lastNum: String,\n      randomPosition: String,\n      hit: Number,\n      miss: Number,\n    };\n  },\n  methods: {\n    checkHit: function (event) {\n      //https://codepen.io/CodifyAcademy/pen/ByBEOz\n      console.log(event.target.id);\n      if (event.target.id !== this.randomPosition){\n        this.hit = false;\n        this.miss = true;\n        console.log(this.hit);\n      }\n      if (event.target.id === this.randomPosition){\n          this.hit = true;\n          this.miss = false;\n          console.log(this.hit);\n          alert(\"nooo me ship!! >:(\");\n        }\n      },\n      randomShip: function(){\n        this.firstNum = Math.floor(Math.random() * 7);\n        this.lastNum = Math.floor(Math.random() * 7);\n        this.randomPosition = this.firstNum + \"-\" + this.lastNum;\n      }\n    },\n    mounted(){\n      this.randomShip();\n    }\n  }\n  </script>\n\n  <style lang=\"less\" scoped>\n  table {\n  display: inline-block;\n\n  td {\n    border: 1px solid green;\n    height: 50px;\n    width: 50px;\n    }\n  }\n\n  h3 {\n    &:first-of-type {\n      letter-spacing: 0.2rem;\n      display: inline-block;\n      writing-mode: vertical-rl;\n      text-orientation: upright;\n    }\n\n    &:last-of-type {\n      letter-spacing: 1.2rem;\n      margin-left: 5rem;\n    }\n  }\n  </style>\n"]}]}